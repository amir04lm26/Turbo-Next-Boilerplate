{
  "name": "snapp-shop",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "cross-env NODE_OPTIONS='--inspect' turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\""
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint-config-custom": "*",
    "stylelint-config-custom": "*",
    "prettier": "^2.7.1",
    "turbo": "^1.3.1"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "browserslist-config-custom": "*"
  },
  "packageManager": "yarn@1.22.17"
}
